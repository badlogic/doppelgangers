{
	"name": "doppelgangers",
	"version": "0.0.2",
	"description": "Find duplicate PRs through embedding visualization",
	"type": "module",
	"bin": {
		"doppelgangers": "./dist/triage.js"
	},
	"scripts": {
		"build": "tsgo",
		"dev": "tsgo --watch",
		"check": "biome check --write . && tsgo --noEmit",
		"prepare": "husky"
	},
	"keywords": [
		"github",
		"pull-request",
		"triage",
		"embeddings",
		"visualization",
		"umap"
	],
	"author": "Mario Zechner",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/badlogic/doppelgangers.git"
	},
	"dependencies": {
		"openai": "^4.104.0",
		"umap-js": "^1.4.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.5",
		"@types/node": "^22.10.5",
		"@typescript/native-preview": "7.0.0-dev.20260120.1",
		"husky": "^9.1.7",
		"typescript": "^5.9.2"
	},
	"engines": {
		"node": ">=20.0.0"
	}
}